{
  "env": {
    "node": true,
    "commonjs": true
  },
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "google", "plugin:prettier/recommended"],
  "rules": {
    "no-console": 0,
    "max-len": ["error", { "code": 120 }],
	"prefer-const": "off",
    "prettier/prettier": [
      "error",
      {
        "doubleQuote": true,
        "tabWidth": 2
      }
    ]
  }
}
